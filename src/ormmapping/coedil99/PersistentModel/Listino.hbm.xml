<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="coedil99.PersistentModel.Listino" table="Listino" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<set name="ORM_Item" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="ListinoID" not-null="false"/>
			<one-to-many class="coedil99.PersistentModel.Item"/>
		</set>
	</class>
</hibernate-mapping>
